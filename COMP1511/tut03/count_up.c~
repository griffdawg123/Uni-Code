// counts up to a user inputted number
// Griffin Doyle (z5311098), 1/02/20\

#include <stdio.h>

int main(void) {
    int num = 1;
    int finish = -1;
    printf("Enter finish: ");
    scanf("%d", finish);
    while(num <= finish) {
        printf("%d\n", num);
        num++;
    }
    return 0;
}
